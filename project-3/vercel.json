{
    "version": 2,
    "buildCommand": "cd client && npm install && npm run dev",
    "outputDirectory": "client/.next",
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "server/src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/src/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }